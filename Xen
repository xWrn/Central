local Tab = Window:NewTab("Gen")
local Section = Tab:NewSection("Gen")

local yellow = Color3.fromRGB(255, 241, 76)

local function addHighlights()
	for i, v in pairs(workspace.Balls:GetChildren()) do
		if v.ClonedBall:FindFirstChild("Highlight") then return end
		local a = Instance.new("Highlight")
		a.Parent = v.ClonedBall
		a.FillColor = yellow
		a.Enabled = false
	end
end

local function enableWhenBlockable()
	for i, v in pairs(workspace.Balls:GetChildren()) do
		if v.ClonedBall:FindFirstChild("Highlight") then
			if v.Blockable.Value then
				v.ClonedBall.Highlight.Enabled = true
			elseif v.Blockable.Value == false then
				v.ClonedBall.Highlight.Enabled = false
			end
		end
	end
end

local function destroyHighlights()
	for i, v in pairs(workspace.Balls:GetChildren()) do
		local highlight = v:FindFirstChild("Highlight")
		if highlight then
			highlight:Destroy()
		end
	end
end

